{
  "name": "tic-tac-dojo",
  "displayName": "Tic Tac Dojo",
  "slug": "tic-tac-dojo",
  "scheme": "mobile",
  "version": "1.0.0",
  "orientation": "portrait",
  "userInterfaceStyle": "automatic",
  "icon": "./assets/icon_set/appstore.png",
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "newArchEnabled": true,
  "jsEngine": "hermes",
  "assetBundlePatterns": [
    "**/*"
  ],
  "android": {
    "icon": "./assets/icon_set/playstore.png",
    "package": "com.tictacdojo",
    "versionCode": 1,
    "adaptiveIcon": {
      "foregroundImage": "./assets/icon_set/playstore.png",
      "backgroundColor": "#191015"
    },
    "allowBackup": false,
    "edgeToEdgeEnabled": true,
    "splash": {
      "image": "./assets/images/logo_bright.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F2937",
      "dark": {
        "image": "./assets/images/logo_bright.png",
        "backgroundColor": "#1F2937"
      }
    }
  },
  "ios": {
    "icon": "./assets/icon_set/appstore.png",
    "supportsTablet": true,
    "bundleIdentifier": "com.tictacdojo",
    "buildNumber": "1",
    "splash": {
      "image": "./assets/images/logo_bright.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F2937",
      "dark": {
        "image": "./assets/images/logo_bright.png",
        "backgroundColor": "#1F2937"
      }
    },
    "privacyManifests": {
      "NSPrivacyAccessedAPITypes": [
        {
          "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
          "NSPrivacyAccessedAPITypeReasons": [
            "CA92.1"
          ]
        }
      ]
    },
    "infoPlist": {
      "ITSAppUsesNonExemptEncryption": false
    }
  },
  "web": {
    "favicon": "./assets/icon_set/appstore.png",
    "bundler": "metro"
  },
  "plugins": [
    "expo-localization",
    "expo-font",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/logo_bright.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#1F2937",
        "dark": {
          "image": "./assets/images/logo_bright.png",
          "backgroundColor": "#1F2937"
        }
      }
    ],
    [
      "react-native-edge-to-edge",
      {
        "android": {
          "parentTheme": "Light",
          "enforceNavigationBarContrast": false
        }
      }
    ]
  ],
  "experiments": {
    "tsconfigPaths": true
  },
  "extra": {
    "ignite": {
      "version": "11.0.1"
    },
    "eas": {
      "projectId": "ba3eb0ed-f58b-4129-b188-c6ef6ea605a0"
    }
  }
}
